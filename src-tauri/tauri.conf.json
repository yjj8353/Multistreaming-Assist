{
  "build": {
    "beforeBuildCommand": "yarn quasar build",
    "beforeDevCommand": "yarn quasar dev",
    "devPath": "http://localhost:9000",
    "distDir": "../dist/spa"
  },
  "package": {
    "productName": "multistreaming-assist-v2",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "path": {
        "all": true
      },
      "window": {
        "all": true
      },
      "dialog": {
        "all": true
      },
      "shell": {
        "all": true
      },
      "fs": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "retrotv.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "decorations": false,
        "fullscreen": false,
        "width": 480,
        "height": 600,
        "resizable": true,
        "title": "Multistreaming Assist V2"
      }
    ]
  }
}
